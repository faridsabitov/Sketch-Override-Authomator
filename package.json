{
  "name": "override-manager",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "override-manager",
    "manifest": "src/manifest.json",
    "main": "override-manager.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faridsabitov/Sketch-Override-Manager.git"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Farid Sabitov <farid.sabitov.design@gmail.com>"
}
